*
	margin 0
	padding 0
body
	background-color #efeff4
.top
	height 350px
	background-color white
	position relative
	.font 
		width 100%
		position absolute
		z-index 1
		color black
		font-weight bold
		text-align center
		margin-top 20px
		.span2
			font-size 22px
			color white
	.ul1
		position absolute
		top 300px
		left 50%
		margin-left -40px
		z-index 1
		li
			list-style none
			height 10px
			width 10px
			background-color white
			border-radius 50%
			float left
			margin-left 10px
	img
		height 350px
		width 100%
		position absolute
		top 0
		left 0
	.input
		position absolute
		top 70px
		left 10px
		right 10px
		background-color white
		z-index 1
		text-align center
		height 30px
		border-radius 5px
		line-height 30px
		color #cfcfcf
		font-size 15px
		font-weight bold
		img
			height 15px
			width 15px
			margin-left 90px
			margin-top 7px
.list
	height 10px
	background-color white
	margin-top 10px
	border-bottom 1px solid #eff0f2
	padding 20px 0px
	line-height 10px
	.list2
		display flex
		div
			flex 1
			text-align center
			border-right 1px solid #eff0f2
			font-size 10px
			color #828282
			.img
				height 20px
				width 20px
				margin-top -5px
			p
				margin-top -5px
			img
				height 5px
				width 7px
				margin-top 5px
